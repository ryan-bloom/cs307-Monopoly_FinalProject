            <InitialPlayerFunds>20,000</InitialPlayerFunds>
            ...
        </Rules>
        <ActionCards>
            <GoToJail>
                <Deck>Community Chest</Deck>
                <Message>You got caught robbing a bank! Go directly to jail!</Message>
                <Holdable>false</Holdable>
            </GoToJail>
            <MoveTo>
                <Deck>Chance</Deck>
